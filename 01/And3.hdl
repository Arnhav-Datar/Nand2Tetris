/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And3 {
    IN a, b, c;
    OUT out;

    PARTS:
    // Put your code here:
				And(a=a,b=b,out=w1);
				And(a=w1,b=c,out=out);
}
