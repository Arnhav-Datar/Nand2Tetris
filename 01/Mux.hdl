/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	And(a=sel,b=b,out=w1);
	Not(in=sel,out=del);
	And(a=a,b=del,out=w2);
	Or(a=w1,b=w2,out=out);
}
